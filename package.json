{
  "name": "react-starter-kit",
  "version": "1.0.0",
  "description": "My take on the React Starter Kit:tm:",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config ./tools/webpack.server.prod.js && webpack -p --config ./tools/webpack.client.prod.js",
    "clean": "rimraf dist .tmp",
    "prebuild": "npm run clean",
    "prestart": "npm run clean && webpack --config tools/webpack.server.dev.js",
    "start": "cross-env NODE_ENV=development node ./.tmp/server.js",
    "start:prod": "cross-env NODE_ENV=production node dist/server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
