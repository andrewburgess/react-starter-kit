{
  "name": "react-starter-kit",
  "version": "1.0.0",
  "description": "My take on the React Starter Kit:tm:",
  "main": "index.js",
  "scripts": {
    "build": "babel ./ --out-dir dist --ignore node_modules,public && copy server/**/*.pug dist && webpack -p --config ./tools/webpack.client.prod.js",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "start": "cross-env NODE_ENV=development node server/index.js",
    "start:prod": "cross-env NODE_ENV=production node dist/server/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrewburgess/react-starter-kit.git"
  },
