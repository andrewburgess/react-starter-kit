  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andrewburgess/react-starter-kit/issues"
  },
  "homepage": "https://github.com/andrewburgess/react-starter-kit#readme",
  "devDependencies": {
    "assets-webpack-plugin": "^3.4.0",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.14.0",
    "copy": "^0.3.0",
