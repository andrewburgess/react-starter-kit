{
  "name": "react-starter-kit",
  "version": "1.0.0",
  "description": "My take on the React Starter Kit :tm:",
  "main": "index.js",
  "scripts": {
    "analyze": "node ./tools/scripts/analyze.js",
    "build": "cross-env NODE_ENV=production webpack --config ./tools/webpack/webpack.production.js --color -p --progress",
    "build:clean": "rimraf ./build",
    "clean": "npm run build:clean && npm run test:clean",
    "generate": "plop --plopfile ./tools/generators/index.js",
    "lint": "npm run lint:js",
    "lint:eslint": "eslint",
    "lint:js": "npm run lint:eslint -- .",
    "lint:staged": "lint-staged",
    "pretest": "npm run test:clean && npm run lint",
    "start": "cross-env NODE_ENV=development node bootstrap.js",
    "start:debug": "cross-env NODE_ENV=development node --debug-brk --inspect bootstrap.js",
    "start:prod": "cross-env NODE_ENV=production node index.js",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "test:clean": "rimraf ./coverage",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrewburgess/react-starter-kit.git"
  },
  "author": "Andrew Burgess <andrew@andrewburgess.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andrewburgess/react-starter-kit/issues"
  },
  "engines": {
    "npm": ">=3",
    "node": ">=6"
  },
  "homepage": "https://github.com/andrewburgess/react-starter-kit#readme",
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-dynamic-import-node": "^1.0.0",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.22.0",
    "babel-plugin-transform-react-constant-elements": "^6.22.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.3.2",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.22.0",
    "circular-dependency-plugin": "^2.0.0",
    "css-loader": "^0.26.1",
    "enzyme": "^2.7.1",
    "eslint": "^3.15.0",
    "eslint-config-revolt": "^1.1.0",
    "file-loader": "^0.10.0",
    "html-loader": "^0.4.4",
    "image-webpack-loader": "^3.2.0",
    "jest-cli": "^18.1.0",
    "lint-staged": "^3.3.0",
    "node-plop": "^0.5.5",
    "null-loader": "^0.1.1",
    "plop": "^1.7.4",
    "pre-commit": "^1.2.2",
    "pug": "^2.0.0-beta11",
    "pug-loader": "^2.3.0",
    "react-addons-test-utils": "^15.4.2",
    "rimraf": "^2.5.4",
    "shelljs": "^0.7.6",
    "sinon": "^1.17.7",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-hot-middleware": "^2.16.1"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "babel-polyfill": "^6.22.0",
    "compression": "^1.6.2",
    "convict": "^2.0.0",
    "cross-env": "^3.1.4",
    "express": "^4.14.1",
    "fontfaceobserver": "^2.0.8",
    "helmet": "^3.4.0",
    "lodash": "^4.17.4",
    "morgan": "^1.8.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-helmet": "^4.0.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.0",
    "redial": "^0.5.0",
    "redux": "^3.6.0",
    "semver": "^5.3.0",
    "styled-components": "^1.4.3",
    "winston": "^2.3.1"
  },
  "lint-staged": {
    "*.js": "lint:eslint"
  },
  "pre-commit": "lint:staged"
}
